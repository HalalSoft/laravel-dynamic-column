{
  "name": "halalsoft/laravel-dynamic-column",
  "description": "Provides a trait for Laravel Eloquent/Query builder to handle MariaDB Dynamic Column.",
  "keywords": [
    "laravel",
    "illuminate",
    "eloquent",
    "mariadb",
    "blob",
    "dynamic-column",
    "dynamic"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Dyas Yaskur",
      "email": "dyas@yaskur.com",
      "homepage": "https://yaskur.net"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/database": "6.*|7.*|8.*|9.*",
    "illuminate/support": "6.*|7.*|8.*|9.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.* || ^9.0",
    "orchestra/testbench": "^4.0 || ^5.0 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Halalsoft\\LaravelDynamicColumn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Halalsoft\\LaravelDynamicColumn\\Tests\\": "tests"
    },
    "classmap": [
      "tests"
    ]
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
